{
  "manifest_version": 3,
  "name": "Time Tracker & Productivity Analysis",
  "version": "1.0",
  "description": "Track time spent on websites and analyze productivity",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/logo1.jpg"
     
    }
  },
  "icons": {
    "16": "icons/logo1.jpg"
  }
}
